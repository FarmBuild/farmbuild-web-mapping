{
  "name": "farmbuild-web-mapping",
  "private": true,
  "version": "0.1.18",
  "description": "This project is the web mapping module of farm build JavaScript library",
  "repository": "https://github.com/FarmBuild/farmbuild-web-mapping",
  "license": "MIT",
  "dependencies": {
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "karma": "^0.12",
    "karma-cli": "^0.0",
    "karma-html2js-preprocessor": "^0.1",
    "karma-json-fixtures-preprocessor": "0.0.1",
    "karma-fixture": "^0.2",
    "karma-chrome-launcher": "^0.1",
    "karma-junit-reporter": "^0.2",
    "jasmine-core": "^2.3",
    "karma-firefox-launcher": "^0.1",
    "karma-jasmine": "^0.3",
    "protractor": "^2.0",
    "http-server": "^0.6",
    "bower": "^1.4",
    "shelljs": "^0.2",
    "minami": "~1.1.0",
    "jsdoc": "^3.3",
    "uglify-js": "2.4.19",
    "jshint": "^2.6",
    "ncp": "git://github.com/bill209/ncp.git#47ac8b01a93b16d403b5a35d66decee83670de17",
    "replace": "^0.3",
    "static-livereload": "^0.1"
  },
  "scripts": {
    "bower-update": "bower update",
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8001 -c-1",
    "livereload": "static-livereload --path .",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "elex": "protractor protractor.conf.js --elementExplorer",
    "protractor": "protractor protractor.conf.js",
    "clean": "rm -rf dist/* & rm -rf docs/*",
    "generate-docs": "node_modules/.bin/jsdoc --configure .jsdoc.json",
    "version": "node_modules/.bin/replace '@version {version}' '@version 0.1.1' src/*.js src/**/*.js",
    "jshint": " node_modules/.bin/jshint -c .jshintrc --filename src/**/*-src.js src/nutrient-calculator.js --verbose --show-non-errors",
    "concat": "uglifyjs src/web-mapping.src.js src/**/*.src.js src/**/**/*.src.js src/index.src.js -o dist/farmbuild-webmapping.js -v -b",
    "uglify": "uglifyjs dist/farmbuild-webmapping.js --source-map dist/farmbuild-webmapping.min.js.map --source-map-url farmbuild-webmapping.min.js.map -o dist/farmbuild-webmapping.min.js -v -c",
    "copy-core": "ncp bower_components/farmbuild-core/dist/ dist/ && ncp bower_components/farmbuild-farmdata/dist/ dist/",
    "copy-ol": "ncp lib/ol/ dist/ol --filter='^((?!debug).)*$'",
    "copy-turf": "ncp bower_components/turf/ dist/turf --filter=.min.js",
    "copy-proj4js": "ncp bower_components/proj4/dist/ dist/proj4 --filter=proj4.js",
    "dist": "npm run clean && npm run generate-docs && npm run concat && npm run uglify && npm run copy-core && npm run copy-ol && npm run copy-turf && npm run copy-proj4js"
  }
}
